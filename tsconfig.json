{
	"compilerOptions": {
		"outDir": "./dist/",
		"declaration": true,
		"sourceMap": true,
		"allowJs": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es6",
		"lib": ["es2019", "dom"],
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"paths": {
			// For examples
			"rorahi": ["src/index"]
		}
	}
}
